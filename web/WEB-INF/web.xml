<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <error-page>
        <error-code>401</error-code>
        <location>/ErrorPage/401.jsp</location>
    </error-page>
    <error-page>
        <error-code>404</error-code>
        <location>/ErrorPage/404.jsp</location>
    </error-page>
    <servlet>
        <servlet-name>Useredit</servlet-name>
        <servlet-class>Functions.Useredit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Delete</servlet-name>
        <servlet-class>Functions.Delete</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getCategories</servlet-name>
        <servlet-class>Data.getCategories</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Useredit</servlet-name>
        <url-pattern>/Useredit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Delete</servlet-name>
        <url-pattern>/Delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getCategories</servlet-name>
        <url-pattern>/getCategories</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
